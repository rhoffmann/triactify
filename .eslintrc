{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "camelcase": 1,
    "complexity": [2, 5],
    "comma-spacing": 1,
    "consistent-return": 0,
    "eol-last": 1,
    "indent": [1, 2],
    "max-depth": [1, 2],
    "max-len": [2, 100],
    "max-nested-callbacks": [1, 1],
    "max-params": [1, 4],
    "max-statements": [1, 15],
    "no-use-before-define": [2, "nofunc"],
    "no-multi-spaces": 1,
    "no-process-exit": 0,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "space-infix-ops": 1,
    "quotes": [1, "single"],
    "semi": [1, "always"],
    "semi-spacing": 1,
    "strict": 0,
    "yoda": [1, "never"]
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "jsx": true,
    "templateStrings": true,
    "modules": true,
    "destructuring": true,
    "defaultParams": true,
    "blockBindings": true
  },
  "globals": {
  }
}
